            do tmpTarget.dispose();
            let i=i+1;
        }
        do targets.dispose();

        let i=0;
        while(i<sizex)
        {
            let tmpArray=map_img[i];
            do tmpArray.dispose();
            let i=i+1;
        }
        do map_img.dispose();
        return;
    }
}